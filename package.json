{
  "name": "snip",
  "author": {"name": "myouo"},
  "publisher": "myouo",
  "displayName": "sublime_snippet",
  "description": "Convert Sublime Text snippets into VS Code snippets.",
  "repository": {
    "type": "git",
    "url": "https://github.com/myouo/sublime_snippet_for_VSCode"
  },
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sublime.convertSnippets"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sublime.convertSnippets",
        "title": "Sublime: Convert Snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.90.0",
    "@types/xml2js": "^0.4.14",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  }
}
